{
    "compilerOptions": {
        "target": "es5",
        "lib": [
            "dom",
            "dom.iterable",
            "esnext",
            "ES2020.Promise"
        ],
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "module": "esnext",
        "moduleResolution": "node",
        "jsx": "react",
        "noImplicitAny": true,
        "skipLibCheck": true,
        "resolveJsonModule": true,
        "downlevelIteration": true,
        "baseUrl": ".",
        "paths": {
            "#types": [
                "src/types"
            ],
            "#components/*": [
                "src/components/*"
            ],
            "#hooks/*": [
                "src/hooks/*"
            ],
            "#redux/*": [
                "src/redux/*"
            ],
            "#types/*": [
                "src/types/*"
            ]
        }
    },
    "include": [
        "src",
        "./server",
        "./test"
    ]
}